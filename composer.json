{
    "name": "beastbytes/leaflet",
    "description": "Leaflet JavaScript mapping library integration",
    "type": "extension",
    "keywords": [
        "Leaflet",
        "map",
        "mapping",
        "maps",
        "yii3"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Chris Yates",
            "email": "chris.l.yates@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "yiisoft/assets": "^2.0",
        "yiisoft/html": "^3.0",
        "yiisoft/widget": "^2.0",
        "npm-asset/leaflet": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest",
        "yiisoft/aliases": "^2.0",
        "yiisoft/cache": "^2.0",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/test-support": "^3.0",
        "yiisoft/view": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Widgets\\Leaflet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
